{
    "name": "danielpk74/laravel-auth-starter",
    "type": "library",
    "description": "A comprehensive Laravel authentication and user management starter package with Vue.js frontend, AdminLTE integration, and role-based access control",
    "keywords": [
        "laravel",
        "authentication",
        "vue",
        "starter",
        "template",
        "adminlte",
        "sanctum",
        "roles",
        "user-management"
    ],
    "license": "MIT",
    "homepage": "https://github.com/danielpk74/laravel-auth-starter",
    "support": {
        "issues": "https://github.com/danielpk74/laravel-auth-starter/issues",
        "source": "https://github.com/danielpk74/laravel-auth-starter"
    },
    "authors": [
        {
            "name": "Daniel PK",
            "email": "danielpk74@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "laravel/sanctum": "^3.2|^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0|^11.0",
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Danielpk74\\LaravelAuthStarter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danielpk74\\LaravelAuthStarter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danielpk74\\LaravelAuthStarter\\LaravelAuthStarterServiceProvider"
            ],
            "aliases": {
                "AuthStarter": "Danielpk74\\LaravelAuthStarter\\Facades\\AuthStarter"
            }
        },
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "pint": "vendor/bin/pint",
        "pint-test": "vendor/bin/pint --test",
        "analyse": "vendor/bin/phpstan analyse",
        "post-autoload-dump": [
            "@php -r \"file_exists('vendor/bin/testbench') && (include 'vendor/bin/testbench') ? include 'vendor/autoload.php' : exit(0);\""
        ]
    }
}
