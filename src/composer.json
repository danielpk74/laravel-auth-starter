{
    "name": "danielpk74/laravel-auth-starter",
    "version": "2.0.1",
    "type": "library",
    "description": "Laravel 12 + Vue 3 authentication starter package with complete auth system, AdminLTE UI, role-based access control, and ready for document monitoring extensions",
    "keywords": [
        "laravel",
        "authentication",
        "vue",
        "starter",
        "template",
        "adminlte",
        "sanctum",
        "roles",
        "user-management"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "danielpk74",
            "email": "danielpk74@gmail.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/danielpk74/laravel-auth-starter",
    "support": {
        "issues": "https://github.com/danielpk74/laravel-auth-starter/issues",
        "source": "https://github.com/danielpk74/laravel-auth-starter"
    },
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/sanctum": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Danielpk74\\LaravelAuthStarter\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danielpk74\\LaravelAuthStarter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danielpk74\\LaravelAuthStarter\\LaravelAuthStarterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "pint": "vendor/bin/pint"
    }
}
