{
    "name": "danielpk74/laravel-auth-starter",
    "type": "library",
    "description": "A comprehensive Laravel authentication and user management starter package with Vue.js frontend, AdminLTE integration, and role-based access control",
    "keywords": [
        "laravel",
        "authentication",
        "vue",
        "starter",
        "template",
        "adminlte",
        "sanctum",
        "roles",
        "user-management"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel PK",
            "email": "danielpk74@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^10.0 || ^11.0 || ^12.0",
        "laravel/sanctum": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0 || ^9.0 || ^10.0",
        "laravel/pint": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Danielpk74\\LaravelAuthStarter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Danielpk74\\LaravelAuthStarter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Danielpk74\\LaravelAuthStarter\\LaravelAuthStarterServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
        "pint": "vendor/bin/pint"
    }
}
